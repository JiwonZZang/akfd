<% if @project.errors.size == 0 %>
	<% if @type == "feeld_up" %>
		$('<%=j render 'project_radio_ajax', project:@project%>').appendTo($("#project_radio_container")).hide().fadeIn('slow')
		$('#project_form form')[0].reset();
	<% else %>
		$('#project_manage_board').after("<%=j render 'projects/project'%>").hide().fadeIn();
	<% end %>
<% else %>
	alert('프로젝트 이름을 작성해 주세요')
<% end %>
